{
  "name": "{{ npmName }}",
  "version": "{{ npmVersion }}",
  "description": "{{ npmDescription }}",
  "author": "{{ npmAuthor }}",
  "repository": {
    "type": "git",
    "url": "https://github.com/apivideo/{{gitRepoId}}"
  },
  "bugs:": "https://github.com/apivideo/{{gitRepoId}}/issues",
  "keywords": [
    "api",
    "video",
    "client",
    "rest"
  ],
  "license": "{{ licenseName }}",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": ["lib/", "doc/", "test/sandbox.spec.ts"],
  "scripts": {
    "prepublishOnly": "tsc",
    "test": "mocha --reporter spec --require ts-node/register --require tsconfig-paths/register test/**/*.spec.ts --exclude test/sandbox.spec.ts",
    "test:sandbox": "mocha --timeout 300000 --reporter spec --require ts-node/register --require tsconfig-paths/register test/sandbox.spec.ts",
    "eslint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.ts\""
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "got": "^11.8.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/chai": "^4.2.17",
    "@types/got": "^9.6.11",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  }
}
